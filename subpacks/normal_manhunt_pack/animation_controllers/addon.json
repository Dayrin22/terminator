{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.setup": {
      "states": {
        "default": {
          "on_entry": [ "/function launcher/setup" ],
          "transitions": [{"stop": "q.is_alive"}]
        },
        "stop": {}
      }
    },
    "controller.animation.speedrunner.death":{
      "states":{
        "alive":{
          "transitions": [{"detect_speedrunner":"query.is_alive"}]
        },
        "detect_speedrunner":{
          "transitions": [{"challenge_incomplete":"!query.is_alive"}]
        },
        "challenge_incomplete":{
          "transitions": [{"alive": "(1.0)"}],
          "on_entry": ["/function system/speedrunnerdeath"]
        }
      }
    },
    "controller.animation.enderdragon.death":{ 
      "states":{ 
        "runtimes":{ 
          "transitions":[{"detect_dragon":"query.is_alive" }]
        },
        "detect_dragon":{ 
          "transitions":[{ "on_challenge_complete":"!query.is_alive" }] 
        }, 
        "on_challenge_complete":{ 
          "transitions":[{"runtimes":"(1.0)"}],
          "on_entry":[ "/function system/enderdragondeath" ]
        } 
      } 
    }
  }
}
